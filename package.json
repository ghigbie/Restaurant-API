{
  "name": "restaurant-api",
  "version": "0.0.1",
  "description": "Restaurant API for RESTful API Course",
  "main": "dist",
  "scripts": {
  	"dev": "NODE_ENV=development nodemon -w src --exec \"babel-node src --presets es2015, stage-0\"",
  	"build": "babel src -s -D -d dist --presets es2015,stage-0"
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig":{
  	"parserOptions":{
  		"ecmaVersion": 7,
  		"sourceType": "module"
  	},
  "env":{
  	"node": true
  	},
  "rules":{
  	"no-console": 0,
  	"no-unused-vars": 1
  	}
  },
  "author": "George Higbie",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "mongoose": "^4.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.17.1",
    "nodemon": "^1.11.0"
  }
}
